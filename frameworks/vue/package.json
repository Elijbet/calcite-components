{
  "name": "@esri/calcite-components-vue",
  "sideEffects": false,
  "version": "0.0.4",
  "private": true,
  "description": "Vue specific wrapper for component-library",
  "repository": {
    "type": "git",
    "url": "https://github.com/ionic-team/ionic.git"
  },
  "scripts": {
    "build": "npm run clean && npm run compile",
    "clean": "rimraf dist",
    "compile": "tsc -p ."
  },
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist/"
  ],
  "devDependencies": {
    "rimraf": "^2.6.3",
    "rollup": "^1.21.2",
    "rollup-plugin-node-resolve": "^5.2.0",
    "typescript": "^3.3.4000",
    "vue": "^3.0.0"
  },
  "dependencies": {
    "@esri/calcite-components": "^1.0.0-beta.46",
    "@stencil/eslint-plugin": "0.3.1"
  },
  "jest": {
    "preset": "ts-jest",
    "setupFilesAfterEnv": [
      "<rootDir>/setupTests.ts"
    ],
    "testPathIgnorePatterns": [
      "node_modules",
      "dist"
    ]
  }
}
